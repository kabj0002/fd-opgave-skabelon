---
//@ts-nocheck
const social_links = await fetch(
  "https://frontend-design-theme.netlify.app/api/employees"
).then((response) => response.json());
console.log("API Response:", social_links);
---


<div class="icons">
    {
      social_links.map((link) => (
        <li>
          <a href={link.url} set:html={link.icon} />
        </li>
      ))
    }
  </div>

  <style>

.icons {
  display: flex;

  a {
    color: aqua;
    cursor: pointer;
  }
}
   
  </style>