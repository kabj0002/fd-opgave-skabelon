---
import "../styles/main.css";
import Layout from "../layouts/Layout.astro";
import RoundHeader from "../components/RoundHeader.astro";
import HeroHeader from "../components/HeroHeader.astro";
import Button from "../components/Button.astro";
import TeamList from "../components/TeamList.astro";

---

<Layout>
    <section class="hero_section">
        <div>
         <HeroHeader class="hero_header" heroText="The Consultors"/>
         <RoundHeader class="yellow_header" headerText="Team"/>
        </HeroHeader>
        </div>
    </section>
<section >
<div class="meet_team">
<h3>Meet Our Great Team</h3>
<div>
<p>Welcome to our amazing team! We are a group of passionate individuals dedicated to excellence and innovation. Each member brings unique skills and experiences, working together to achieve our goals. Join us as we embark on exciting projects and create lasting impacts.</p>
<Button class="default_black" buttonText="Join our team"/>
</div>
</section>
