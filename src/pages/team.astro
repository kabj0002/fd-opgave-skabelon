</Layout>

<style>
    .our_team {
display: flex;
flex-direction: column;
min-height: 70svh;
margin: 5rem 0 0 5rem;

h2 {
    line-height: 4rem;
}
* + * {
    margin: 1rem 0 1rem 0;
}
.team {
    display: grid;
    grid-template-columns:  1fr 1fr 1fr;
    align-items: center;
    gap: 30px;
    text-align: center;

}
img {
    border-radius: 20px;
}
.some {
    display: flex;
    justify-content: space-evenly;
}
}
.hero_section {
display: grid;
grid-column: full;
background-image: url("../src/imgs/7ec1cd621b3d0aa1b2858eadb0155a72.webp");
height: 400px;
background-position: end;
background-size: cover;
/* justify-items: center; */

.hero_header {
    position: relative;
    top: 5rem;
    left: 5rem; 
    margin-left: 8rem;
}
.yellow_header {
 position: relative;
 margin: -6rem 0 0 16rem;
}
/* .yellow_header {
position: inherit;
} */
/* div {
    display: flex;
    position: absolute;
    grid-column: content; */
}
.meet_team {
    display: grid;
    grid-template-columns:  1fr 1fr;
    margin: 3rem 0 0 5rem;
    

    div {
        display: flex;
        grid-row: 2/3;
        grid-column: span 2;
        justify-content: space-between;
        
    }
}

</style>